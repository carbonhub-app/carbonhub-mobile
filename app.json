{
  "expo": {
    "name": "CarbonHub",
    "slug": "carbon-hub-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/icons/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carbonhub.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/icons/adaptive-icon.png",
        "backgroundColor": "#10b981"
      },
      "package": "com.carbonhub.mobile"
    },
    "web": {
      "favicon": "assets/icons/favicon.png",
      "bundler": "metro"
    },
    "scheme": "carbonhub",
    "plugins": [
      "expo-router"
    ]
  }
}
